{
    "name": "Historic Tab",
    "version": "1.0",
    "manifest_version": 2,
    "description": "A history focused replacement for the chrome new tab page",
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": ["history"]
}
